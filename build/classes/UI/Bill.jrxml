	<queryString>
		<![CDATA[SELECT
     sales.`SaleID` AS sales_SaleID,
     sales.`CustomerID` AS sales_CustomerID,
     sales.`Cash` AS sales_Cash,
     sales.`LoyaltyPoints` AS sales_LoyaltyPoints,
     sales.`Discount` AS sales_Discount,
     sales.`subtotal` AS sales_subtotal,
     sales.`TotalPrice` AS sales_TotalPrice,
     sales.`Balance` AS sales_Balance,
     sales.`Date` AS sales_Date,
     salesdetails.`ID` AS salesdetails_ID,
     salesdetails.`CustomerID` AS salesdetails_CustomerID,
     salesdetails.`Item` AS salesdetails_Item,
     salesdetails.`UnitPrice` AS salesdetails_UnitPrice,
     salesdetails.`Quantity` AS salesdetails_Quantity,
     salesdetails.`Amount` AS salesdetails_Amount
FROM
     `sales` sales INNER JOIN `salesdetails` salesdetails ON sales.`SaleID` = salesdetails.`ID`
WHERE sales.`SaleID` = $P{OrderID}]]>
	</queryString>
